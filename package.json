  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --ci --coverage --watchAll=false",
    "test:api": "jest --testPathPatterns=\"src/__tests__/api/\" --setupFilesAfterEnv ./src/__tests__/setup.ts --testEnvironment=node --verbose",
    "test:api:watch": "npm run test:api -- --watch",
    "test:api:coverage": "npm run test:api -- --coverage",
    "test:components": "jest --testPathPatterns=\"src/__tests__/components/\" --setupFilesAfterEnv ./jest.setup.js",
    "test:e2e": "playwright test",
    "test:unit": "jest --testPathPatterns=\"src/__tests__/(helpers|utils)/\" --setupFilesAfterEnv ./src/__tests__/setup.ts --testEnvironment=node",
    "test:integration": "npm run test:api && npm run test:components",
    "test:all": "npm run test:unit && npm run test:integration",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "performance-budget": "node scripts/enforce-performance-budget.js",
    "lighthouse-ci": "lhci autorun",
    "performance-check": "npm run build && npm run performance-budget && npm run lighthouse-ci",
    "production-ready": "node scripts/production-readiness-check.js",
    "reset-database": "psql $DATABASE_URL -f scripts/reset-database-for-production.sql",
    "generate-types": "npx supabase gen types typescript --project-id vwejbgfiddltdqwhfjmt > src/lib/db/database.types.ts"
